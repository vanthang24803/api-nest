    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.4.7",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/terminus": "^10.2.3",
    "@nestjs/typeorm": "^10.0.2",
    "axios": "^1.7.9",
    "base64-js": "^1.5.1",
    "bcrypt": "^5.1.1",
    "bull": "^4.16.4",
    "cache-manager": "^3.6.0",
