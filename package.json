    "lodash": "^4.17.21",
    "mime-types": "^2.1.35",
    "minio": "^8.0.2",
    "multer": "1.4.5-lts.1",
    "nest-winston": "^1.9.7",
    "nestjs-form-data": "^1.9.92",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "sharp": "^0.33.5",
